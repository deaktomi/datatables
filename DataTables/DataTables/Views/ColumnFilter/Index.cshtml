@{
    Layout = null;
}

<!DOCTYPE html>
<html>    
<head>   
    <title>jQuery DataTables/ASP.NET MVC Filtering</title>
    <link href="~/Content/dataTables/demo_table.css" rel="stylesheet" type="text/css" />        
    <script src="~/Scripts/jQuery-1.4.4.min.js" type="text/javascript"></script>
    <script src="~/Scripts/jQuery.dataTables.min.js" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.dataTables.columnFilter.js")" type="text/javascript"></script>
    <script src="~/Scripts/init.js" type="text/javascript"></script>

</head>
<body>
<table id="myDataTable" class="display">
    <thead>
    <tr>
        <th>ID</th>
        <th>Company name</th>
        <th>Town</th>
        <th>Date Created</th>
    </tr>
    </thead>
    <tbody> 
    </tbody>
    <tfoot>
    <tr>
        <th>ID</th>
        <th>Company name</th>
        <th>Town</th>
        <th>Date Created</th>
    </tr>
    </tfoot>
</table>
</body>
</html>