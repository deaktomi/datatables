@{
    Layout = null;
}

<!DOCTYPE html>
<html>
    <head>
        <title>Customization of ASP.NET MVC DataTable using DataTable Editable plugin</title>
        <link href="@Url.Content("~/Content/dataTables/demo_table.css")" rel="stylesheet" type="text/css" />
        <script src="@Url.Content("~/Scripts/jquery-1.4.4.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/jquery.dataTables.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/jquery.jeditable.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/jquery-ui.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/jquery.validate.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/jquery.dataTables.editable.js")" type="text/javascript"></script>
        @RenderSection("head", required: false)
    </head>
    <body>
        <div id="container">
            <a href="/Editable/Index">Basic Example</a>
            <a href="/Editable/Ajax">Getting data with Ajax</a>
            <a href="/Editable/Customization">Customization</a>
            <a href="/Editable/IndividualColumnEditing">Individual Column Editing</a>
            @RenderBody()
        </div>
    </body>
</html>
